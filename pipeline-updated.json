{
    "pipeline": {
        "name": "rockops-backend-pipeline",
        "roleArn": "arn:aws:iam::490224159443:role/CodePipelineServiceRole",
        "artifactStore": {
            "type": "S3",
            "location": "rockops-pipeline-artifacts-490224159443"
        },
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
